<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Brush.Dark" Color="#FF1E2336"/>
    <SolidColorBrush x:Key="Brush.Middle" Color="#FF2A3046"/>
    <SolidColorBrush x:Key="Brush.Light" Color="#FF49A2FE"/>
    <LinearGradientBrush x:Key="Brush.Gradual" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Color="#FF334ABB" Offset="0"/>
        <GradientStop Color="#FF21B9DF" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="Brush.Gray" Color="#FF9C9C9A"/>

    <LinearGradientBrush x:Key="Brush.NavSelector" StartPoint="0,0.5" EndPoint="1,0.5">
        <GradientStop Color="#FF2A3046" Offset="0"/>
        <GradientStop Color="#FF49A2FE" Offset="0.5"/>
        <GradientStop Color="#FF2A3046" Offset="1"/>
    </LinearGradientBrush>
</ResourceDictionary>